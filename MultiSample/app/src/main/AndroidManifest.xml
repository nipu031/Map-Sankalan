<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sau.multisample">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sau.multisample.Gesture"></activity>
        <activity android:name="com.example.sau.multisample.Selection"></activity>
        <activity android:name="com.example.sau.multisample.ThreeIntoThree"></activity>
        <activity android:name="com.example.sau.multisample.TwoIntoOne"></activity>
        <activity android:name="com.example.sau.multisample.FourIntoThree"></activity>
        <activity android:name="com.example.sau.multisample.TwoIntoTwo"></activity>
        <activity android:name="com.example.sau.multisample.TwoIntoThree"></activity>
    </application>

</manifest>