<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nipu.example">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.nipu.example.Selection" />

        <activity android:name="com.example.nipu.example.TwoIntoOne" />
        <activity android:name="com.example.nipu.example.TwoIntoTwo" />

        <activity android:name="com.example.nipu.example.TwoIntoThree" />

        <activity android:name="com.example.nipu.example.FourIntoThree" />

        <activity android:name="com.example.nipu.example.ThreeIntoThree" />
    </application>

</manifest>